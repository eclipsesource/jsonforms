{
  "name": "@jsonforms/material-renderers",
  "version": "3.1.0-alpha.1",
  "description": "Material Renderer Set for JSON Forms",
  "repository": "https://github.com/eclipsesource/jsonforms",
  "bugs": "https://github.com/eclipsesource/jsonforms/issues",
  "homepage": "http://jsonforms.io/",
  "license": "MIT",
  "directories": {
    "example": "example",
    "src": "src",
    "webpack": "webpack"
  },
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "material",
    "renderer",
    "form",
    "forms",
    "json",
    "jsonforms",
    "frontend",
    "generator",
    "input",
    "renderengine",
    "jsonschema",
    "schema",
    "uischema",
    "layout",
    "customization"
  ],
  "main": "lib/jsonforms-react-material.cjs.js",
  "module": "lib/jsonforms-react-material.esm.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:examples-app": "rollup -c rollup.example.config.js",
    "dev": "webpack --config webpack/webpack.dev.js && webpack-dev-server --config webpack/webpack.dev.js --env=dev --inline",
    "clean": "rimraf lib coverage dist .nyc_output example/dist 2> /dev/null",
    "lint": "tslint --project tsconfig.json --exclude src/models/jsonSchema.ts",
    "test": "jest --no-cache",
    "test-cov": "jest --no-cache --coverage",
    "report": "nyc report --reporter=html",
    "doc": "typedoc --name 'JSON Forms React Material Renderers'  --excludeExternals  --out docs src"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.tsx"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    }
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "dependencies": {
    "@date-io/dayjs": "1.3.13",
    "dayjs": "1.11.7"
  },
  "peerDependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@jsonforms/core": "3.1.0-alpha.1",
    "@jsonforms/react": "3.1.0-alpha.1",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.8.6",
    "@mui/x-date-pickers": "^6.1.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@jsonforms/core": "^3.1.0-alpha.1",
    "@jsonforms/react": "^3.1.0-alpha.1",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.8.6",
    "@mui/x-date-pickers": "^6.1.0",
    "@rollup/plugin-commonjs": "^23.0.3",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/enzyme": "^3.10.3",
    "@types/react-dom": "^17.0.9",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "copy-webpack-plugin": "^11.0.0",
    "enzyme": "^3.10.0",
    "jest": "^26.6.3",
    "jsonforms-typedoc-theme": "^1.0.1",
    "nyc": "^15.1.0",
    "react-dom": "^17.0.2",
    "rimraf": "^4.4.1",
    "rollup": "^2.78.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-import-css": "^3.1.0",
    "rollup-plugin-visualizer": "^5.4.1",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^26.4.4",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-loader": "^3.5.4",
    "typedoc": "^0.24.1",
    "typescript": "4.8.4",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  }
}

{
  "name": "@jsonforms/editor",
  "version": "2.0.0",
  "description": "JSON Forms Editor",
  "repository": "https://github.com/eclipsesource/jsonforms",
  "bugs": "https://github.com/eclipsesource/jsonforms/issues",
  "homepage": "http://jsonforms.io/",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "example": "example",
    "src": "src",
    "webpack": "webpack"
  },
  "main": "dist/ts-build/jsonforms-editor.js",
  "typings": "dist/ts-build/jsonforms-editor.d.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack --config webpack/webpack.dev.js && webpack-dev-server --config webpack/webpack.dev.js --env=dev --inline",
    "bundle": "webpack --config webpack/webpack.build.js --env=production --display-error-details",
    "build": "npm run tsc && ncp ./jsoneditor.css dist/jsoneditor.css && ncp ./jsoneditor.materialize.css dist/jsoneditor.materialize.css",
    "tsc": "tsc",
    "lint": "tslint --project tslint.json",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@jsonforms/core": "2.0.0",
    "@jsonforms/react": "2.0.0",
    "@jsonforms/webcomponent": "2.0.0",
    "ajv": "^4.11.2",
    "json-refs": "^2.1.6",
    "lodash": "^4.17.4",
    "react": "^16.2.0",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@jsonforms/material-renderers": "2.0.0",
    "@jsonforms/vanilla-renderers": "2.0.0",
    "awesome-typescript-loader": "^3.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "ncp": "^2.0.0",
    "source-map-loader": "^0.1.6",
    "tslint": "^5.6.0",
    "typescript": "^2.4.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.7.1"
  }
}
